{
  "name": "biekerudan/pathfinder_esi",
  "description": "ESI API library for Pathfinder",
  "type": "library",
  "homepage": "https://github.com/BiekerUdan/pathfinder_esi",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Sam O'Neill",
      "email": "samuel.p.oneill@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Exodus4D\\ESI\\": "app/",
      "Exodus4D\\Pathfinder\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Exodus4D\\ESI\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.4",
    "ext-json": "*",
    "bcosca/fatfree-core": "^3.7",
    "psr/cache": "^1.0",
    "guzzlehttp/guzzle": "^6.5",
    "caseyamcl/guzzle_retry_middleware": ">=2.13.0",
    "cache/void-adapter": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "vlucas/phpdotenv": "^5.2"
  },
  "config": {
    "optimize-autoloader": true,
    "lock": false,
    "platform-check": false
  }
}
